<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Stasevich353502.UI.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:Stasevich353502.UI"
    xmlns:pages="clr-namespace:Stasevich353502.UI.Pages"
    >
    
    <ShellContent
        Title="Home"
        Icon="{FontImageSource FontFamily='IcoFont', Glyph='&#xef47;', Size=30, Color=Tomato}"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    
    <ShellContent
        Title="Sushi sets list"
        Icon="{FontImageSource FontFamily='IcoFont', Glyph='&#xeb8b;', Size=30, Color=Tomato}"
        ContentTemplate="{DataTemplate pages:SushiSets}"
        Route="SushiSets" />

</Shell>
